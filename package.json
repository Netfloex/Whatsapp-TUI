		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@rollup/plugin-run": "^2.1.0",
		"@trivago/prettier-plugin-sort-imports": "^3.1.1",
		"@types/luxon": "^2.0.9",
		"@types/node": "^17.0.8",
